<h1>User info</h1>
<%= form_with model:@usr,local:true do |f| %>
  <label for="user_name">Name</label>
  <%= f.text_field :name , autofocus:true,id:"user_name",placeholder:"名前"%>
  <label for="user_image">Image</label>
  <%= f.attachment_field:image%>
  <label for "user_introduction">introduction</label>
  <%= f.text_field :introduction , autofocus:true,id:"user_introduction",placeholder:"自己紹介文"%>
  <%=f.submit "Update User"%>
<%end%>